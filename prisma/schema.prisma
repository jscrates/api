generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model packages {
  id       String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  name     String
  versions Json[]
}

model users {
  id       String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  password String
  username String
}
